
fn main() -> i32 {
    add(1, 1)
}

fn add(comptime a: i32, comptime b: i32) -> i32 {
    struct {
        fn run() -> i32 {
            struct {
                fn run2() -> i32 {
                    struct {
                        fn run3() -> i32 {
                            a + b
                        }
                    }.run3()
                }
            }.run2()
        }
    }.run()
}


